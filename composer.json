{
  "require": {
    "craftcms/cms": "^4.4.0",
    "nystudio107/craft-seomatic": "4.0.23",
    "nystudio107/craft-vite": "4.0.5",
    "trendyminds/craft-palette": "3.1.3",
    "verbb/navigation": "2.0.17",
    "vlucas/phpdotenv": "^5.4.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/generator": "^1.3",
    "craftcms/phpstan": "dev-main",
    "friendsoftwig/twigcs": "^6.2",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "ddev setup"
    ],
    "test": "ecs check --ansi && phpstan --memory-limit=1G && twigcs",
    "fix": "ecs check --ansi --fix"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
